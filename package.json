{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "leju-kpi-mark",
  "title": "leju_kpi_mark",
  "description": "quick create mark for group users",
  "icon": "command-icon.png",
  "author": "carlos-wong",
  "license": "MIT",
  "commands": [
    {
      "name": "gc",
      "title": "gc",
      "description": "create kpi mark comment ",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "instance",
      "type": "textfield",
      "required": true,
      "title": "GitLab URL",
      "description": "URL of your GitLab instance",
      "placeholder": "URL of your GitLab instance e.g. https://gitlab.com"
    },
    {
      "name": "token",
      "type": "password",
      "required": true,
      "title": "API Token",
      "description": "Your GitLab API token. `api` scope for read and write operations, `read_api` when you just want to read only commands.",
      "link": "https://docs.gitlab.com/ee/user/profile/personal_access_tokens.html",
      "placeholder": "Enter your GitLab API token."
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.24.5",
    "@apollo/client": "^3.4.8",
    "apollo-link-context": "^1.0.20",
    "fuse.js": "^6.4.6",
    "graphql": "^15.5.1",
    "node-fetch": "^2.6.1",
    "open": "^8.2.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/node-fetch": "^2.5.12",
    "@types/open": "^6.2.1",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
